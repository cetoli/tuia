<div id="content">
	<div>
		<div class="content">
			<h1 class="title style1">Adicionar Resenha de Artigo</h1>
			<br/>
			<% form_tag({:action => 'create'}, :multipart => true) do %>
				<%= render :partial => 'form' %>
				<%= submit_tag "Cria nova resenha de artigo" %>
			<% end %>
			<p><%= link_to image_tag("goBack.gif", :border => 0, :alt => 'Voltar'), {:action => 'list'} %></p>
	  	</div>
	</div>
</div>
