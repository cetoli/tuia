<%= error_messages_for 'documento' %>

<!--[form:documento]-->
<p>
	<label for="documento_titulo">Título:</label>
	<br/>
	<%= text_field 'documento', 'titulo', :size => 75, :maxlength => 255 %>
</p>
<p>
	<label for="documento_resenha">Resenha:</label>
	<br/>
	<%= text_area 'documento', 'resenha', :cols => 57 %>
</p>
<p>
	<label for="documento_anexos">Anexo:</label>
	<br/>
	<%= file_field 'documento', 'anexos' %>
</p>
<p>
	<label for="documento_artigos">Artigo:</label>
	<br/>
	<%= file_field 'documento', 'artigos' %>
</p>
