<div id="content">
	<div>
		<div class="content">
			<h1 class="title style1">Adicionar Plataforma</h1>
			<br/>
			<%= start_form_tag({:action => 'create'}) %>
			<%= render :partial => 'form' %>
			<%= submit_tag "Cria nova plataforma" %>
			<%= end_form_tag %>
			<p><%= link_to image_tag("goBack.gif", :border => 0, :alt => 'Voltar'), {:action => 'list'} %></p>
	  	</div>
	</div>
</div>
