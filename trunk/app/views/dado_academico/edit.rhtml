			<br/>
			<%= link_to image_tag("opn.png", :border => 0, :alt => 'Mostrar'), :controller => 'user', :action => 'show', :id => @user_id %> | <%= link_to image_tag("goBack.gif", :border => 0, :alt => 'Voltar'), :controller => 'user', :action => 'list' %>
	  	</div>
	</div>
</div>
