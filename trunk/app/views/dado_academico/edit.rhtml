<div id="content">
	<div>
		<div class="content">
			<h1 class="title style1">Editar Cadastro Profissional</h1>
			<br/>
			<% form_tag :action => 'update', :id => @dadoAcademico do %>
				<%= render :partial => 'form' %>
				<%= submit_tag 'Alterar' %>
