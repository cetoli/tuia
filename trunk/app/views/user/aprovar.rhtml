<div id="content">
	<div>
		<div class="content">
			<h1 class="title style1">Aprovar Usuário</h1>
			<br/>
			<% form_tag :action => 'doAprove', :id => @user do %>
			  <%= render :partial => 'formUsr' %>
			  <%= submit_tag 'Aprovar' %>
			<% end %>
			<%= link_to image_tag("goBack.gif", :border => 0, :alt => 'Voltar'), :action => 'show', :id => @user %>
	  	</div>
	</div>
</div>
